<%= render 'components/breadcrumb', breadcrumbs: [
  { text: 'Home', path: root_path},
  { text: 'Menus', path: restaurant_menus_path(@restaurant)},
  { text: @menu.menu_name, path: restaurant_menu_path(@restaurant, @menu)},
  { text: 'Edit', path: edit_restaurant_menu_path(@restaurant, @menu) }
] %>

<div class='p-10'>
  <h4 class='font-semibold mb-5 uppercase underline'>Edit menu</h4>
  <%= render 'form', menu: @menu %>
</div>


